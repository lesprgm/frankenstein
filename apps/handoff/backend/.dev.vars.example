# Copy this file to .dev.vars and fill in your values
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_key
VECTORIZE_ACCOUNT_ID=your_vectorize_account_id
VECTORIZE_API_TOKEN=your_vectorize_api_token
OPENAI_API_KEY=your_openai_api_key

# OpenAI Models (optional - defaults shown)
OPENAI_CHAT_MODEL=gpt-5-nano
OPENAI_EMBEDDING_MODEL=text-embedding-3-small
OPENAI_EXTRACTION_MODEL_PERSONAL=gpt-5-nano
OPENAI_EXTRACTION_MODEL_TEAM=gpt-5-nano

JWT_SECRET=your_jwt_secret

# Optional MAKER overrides (if you want to tweak reliability layer)
MAKER_ENABLED=true
MAKER_MODEL=gemini-2.0-flash-lite
MAKER_REPLICAS=3
MAKER_TEMPERATURE=0.4